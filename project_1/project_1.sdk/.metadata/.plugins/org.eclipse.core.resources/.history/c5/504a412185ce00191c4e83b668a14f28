#include "Rectangle.h"

Rectangle::Rectangle(int SideS, u16 number,int xmin,int ymin,int side,u16 rgb,int speed)
: Square(number, xmin, ymin, side, rgb, speed) {
	Rectangle::SideS = SideS;
}

void Rectangle::Draw(void){
	Square::Draw();
	VGA_SQUAREDRAW_mWriteReg(VGA_DEVICE, REG3_OFFSET, Rectangle::X_min + Rectangle::SideS);
}



